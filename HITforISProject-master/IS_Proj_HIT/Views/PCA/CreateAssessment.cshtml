@model AssessmentFormPageModel;
@{
    ViewData["Title"] = "Patient Care Assessment";
}

<form class="col" method="post" id="pca" name="pca" asp-action="SaveAssessment">
    <nav class="navbar navbar-expand-lg navbar-dark bg-secondary nav-fill w-100 widely">
        <ul class="navbar-nav nav-fill w-100">
            <li class="nav-item menu-item">
                <button class="btn btn-danger" type="button">
                    <h4>Patient:&nbsp;@ViewBag.Patient.FirstName&nbsp;@ViewBag.Patient.MiddleName&nbsp;@ViewBag.Patient.LastName</h4>
                </button>
            </li>
            <li class="nav-item menu-item dob patientBanner">
                <span class="heading">DOB: </span> @ViewBag.Patient.Dob.ToShortDateString()
            </li>
            <li class="nav-item menu-item patientBanner">
                <span class="heading">Age: </span> @ViewBag.Encounter.PatientCurrentAge
            </li>
            <li class="nav-item menu-item patientBanner">
                <span class="heading">MRN: </span>@(ViewBag.Encounter.Mrn ?? string.Empty)
            </li>
        </ul>
    </nav>
    <input type="hidden" asp-for="EncounterId" value="@ViewBag.Encounter.EncounterId" />
    <input type="hidden" asp-for="PatientMrn" value="@ViewBag.Patient.Mrn" />
    <div class="tab-content">
            <!-- General tab -->
        <div class="tab-pane active" id="general" role="tabpanel" aria-labelledby="general-tab">

            <div class="card">
                <div class="card-header bg-primary text-white">
                    <h4>Patient Care Assessment - General Systems <span class="float-right">Page 1</span></h4>
                </div>
                <div class="card-body">
                    <div class="form-group row">
                        <div class="col-sm-12 col-lg-3 ">
                            <button class="btn btn-primary m-r" type="button">
                                <i class="fas fa-info"></i>
                            </button>
                            <label class="form-label" for="inputTemperature">Temperature</label>
                        </div>
                        <div class="col-sm-12 col-lg-4">
                            <input type="number" class="form-control" id="inputTemperature" placeholder="Temperature"
                                   step="0.01" max="999.99" asp-for="Temperature">
                        </div>
                        <div class="col-sm-12 col-lg-5">
                            <input class="form-control" placeholder="Notes" type="text" />
                        </div>
                    </div>
                    <div class="form-group row">
                        <div class="col-sm-12 col-lg-3"></div>
                        <div class="col-sm-6 col-lg-2 ">
                            <select class="form-control" asp-items="@ViewBag.TempUnits" asp-for="TempUnit"></select>
                        </div>
                        <div class="col-sm-6 col-lg-2 ">
                            <select class="form-control" asp-items="@ViewBag.TempRoutes" asp-for="TempRouteTypeId"></select>
                        </div>
                        <div class="col-sm-12 col-lg"></div>
                    </div>
                    <hr />
                    <div class="form-group row">
                        <div class="col-sm-12 col-lg-3 ">
                            <button class="btn btn-primary m-r" type="button">
                                <i class="fas fa-info"></i>
                            </button>
                            <label class="form-label" for="inputWeight">Weight</label>
                        </div>
                        <div class="col-sm-12 col-lg-4">
                            <input type="number" class="form-control" id="inputWeight" placeholder="Weight"
                                   min="0" step="0.01" asp-for="Weight">
                        </div>
                        <div class="col-sm-12 col-lg-5">
                            <input class="form-control" placeholder="Notes" type="text" />
                        </div>
                    </div>
                    <div class="form-group row">
                        <div class="col-sm col-lg-3"></div>
                        <div class="col-sm-12 col-lg-2 ">
                            <select class="form-control" asp-items="@ViewBag.WeightUnits" asp-for="WeightUnit"></select>
                        </div>
                        <div class="col-sm col-lg"></div>
                    </div>
                    <hr />
                    <div class="form-group row">
                        <div class="col-sm-12 col-lg-3 ">
                            <button class="btn btn-primary m-r" type="button">
                                <i class="fas fa-info"></i>
                            </button>
                            <label class="form-label" for="inputHeight">Height/Length</label>
                        </div>
                        <div class="col-sm-12 col-lg-4">
                            <input type="number" class="form-control" id="inputHeight" placeholder="Height/Length"
                                   min="0" step="0.01" asp-for="Height">
                        </div>
                        <div class="col-sm-12 col-lg-5">
                            <input class="form-control" placeholder="Notes" type="text" />
                        </div>
                    </div>
                    <div class="form-group row">
                        <div class="col-sm col-lg-3"></div>
                        <div class="col-sm-12 col-lg-2 ">
                            <select class="form-control" asp-items="@ViewBag.LengthUnits" asp-for="HeightUnit"></select>
                        </div>
                        <div class="col-sm col-lg"></div>
                    </div>
                    <hr />
                    <div class="form-group row">
                        <div class="col-sm-12 col-lg-3 ">
                            <button class="btn btn-primary m-r" type="button">
                                <i class="fas fa-info"></i>
                            </button>
                            <label class="form-label" for="inputHead">Head Circumference</label>
                        </div>
                        <div class="col-sm-12 col-lg-4">
                            <input type="number" class="form-control" id="inputHead" placeholder="Head Circ."
                                   min="0" step="0.01" asp-for="HeadCircumference">
                        </div>
                        <div class="col-sm-12 col-lg-5">
                            <input class="form-control" placeholder="Notes" type="text" />
                        </div>
                    </div>
                    <div class="form-group row">
                        <div class="col-sm col-lg-3"></div>
                        <div class="col-sm-12 col-lg-2 ">
                            <select class="form-control" asp-items="@ViewBag.LengthUnits" asp-for="HeadCircUnit"></select>
                        </div>
                        <div class="col-sm col-lg"></div>
                    </div>
                    <hr />
                    <div class="form-group row">
                        <div class="col-sm-12 col-lg-3 ">
                            <button class="btn btn-primary m-r" type="button">
                                <i class="fas fa-info"></i>
                            </button>
                            <label class="form-label" for="inputBMI">Body Mass Index</label>
                        </div>
                        <div class="col-sm-12 col-lg-4">
                            <input type="number" class="form-control" id="inputBMI" placeholder="BMI"
                                   min="0" step="0.01" asp-for="BodyMassIndex">
                        </div>
                        <div class="col-sm-12 col-lg-5">
                            <input class="form-control" placeholder="Notes" type="text" />
                        </div>
                    </div>
                    <div class="form-group row">
                        <div class="col-sm col-lg-3"></div>
                        <div class="col-sm-12 col-lg-2">
                            <select class="form-control">
                                <option>Method</option>
                            </select>
                        </div>
                        <div class="col-sm col-lg"></div>
                    </div>
                    <hr />
                    <div class="form-group row">
                        <div class="col-sm-12 col-lg-3">
                            <button class="btn btn-primary m-r" type="button">
                                <i class="fas fa-info"></i>
                            </button>
                            <label class="form-label" for="inputSystolic">Systolic / Diastolic</label>
                        </div>
                        <div class="col-sm-12 col-lg-2">
                            <input type="number" class="form-control" id="inputSystolic" placeholder="Systolic"
                                   min="0" step="0.01" asp-for="SystolicBloodPressure">
                        </div>
                        <div class="col-sm-12 col-lg-2">
                            <input type="number" class="form-control" id="inputDiastolic" placeholder="Diastolic"
                                   min="0" step="0.01" asp-for="DiastolicBloodPressure">
                        </div>
                        <div class="col-sm-12 col-lg-5">
                            <input class="form-control" placeholder="Notes" type="text" />
                        </div>
                    </div>
                    <div class="form-group row">
                        <div class="col-sm-12 col-lg-3">
                        </div>
                        <div class="col-sm-4 col-lg-2">
                            <select class="form-control" asp-items="@ViewBag.BpLocation" asp-for="BpLocationUnit"></select>
                        </div>
                        <div class="col-sm-4 col-lg-2">
                            <select class="form-control" asp-items="@ViewBag.BpMethod" asp-for="BpMethodUnit"></select>
                        </div>
                        <div class="col-sm-4 col-lg-2">
                            <select class="form-control">
                                <option>Position</option>
                            </select>
                        </div>
                    </div>
                    <hr />
                    <div class="form-group row">
                        <div class="col-sm-12 col-lg-3">
                            <button class="btn btn-primary m-r" type="button">
                                <i class="fas fa-info"></i>
                            </button>
                            <label for="inputPulse" class="form-label">Pulse</label>
                        </div>
                        <div class="col-sm-12 col-lg-4">
                            <input type="number" class="form-control" id="inputPulse" placeholder="Pulse"
                                   min="0" step="0.01" asp-for="Pulse">
                        </div>
                        <div class="col-sm-12 col-lg-5">
                            <input class="form-control" placeholder="Notes" type="text" />
                        </div>
                    </div>
                    <div class="form-group row">
                        <div class="col-sm-12 col-lg-3"></div>
                        <div class="col-sm-4 col-lg-2">
                            <select class="form-control">
                                <option>Method</option>
                            </select>
                        </div>
                        <div class="col-sm-4 col-lg-2">
                            <select class="form-control">
                                <option>Location</option>
                            </select>
                        </div>
                        <div class="col-sm-4 col-lg-2">
                            <select class="form-control">
                                <option>Position</option>
                            </select>
                        </div>
                    </div>
                    <hr />
                    <div class="form-group row">
                        <div class="col-sm-12 col-lg-3">
                            <button class="btn btn-primary m-r" type="button">
                                <i class="fas fa-info"></i>
                            </button>
                            <label class="form-label" for="inputPulseOxi">Pulse Oximentry</label>
                        </div>
                        <div class="col-sm-12 col-lg-4">
                            <input type="number" class="form-control" id="inputPulseOxi" placeholder="Pulse Oxim"
                                   min="0" step="0.01" asp-for="PulseOximetry">
                        </div>
                        <div class="col-sm-12 col-lg-5">
                            <input class="form-control" placeholder="Notes" type="text" />
                        </div>
                    </div>
                    <div class="form-group row">
                        <div class="col-sm-12 col-lg-3">
                        </div>
                        <div class="col-sm-4 col-lg-2">
                            <select class="form-control">
                                <option>Method</option>
                            </select>
                        </div>
                        <div class="col-sm-4 col-lg-2">
                            <input class="form-control" id="inputPulseOxi" placeholder="O2 %" type="number">
                        </div>
                        <div class="col-sm-4 col-lg-2">
                            <input class="form-control" id="inputPulseOxi" placeholder="Flow Rate" type="number">
                        </div>
                    </div>
                    <hr />
                    <div class="form-group row">
                        <div class="col-sm-12 col-lg-3">
                            <button class="btn btn-primary m-r" type="button">
                                <i class="fas fa-info"></i>
                            </button>
                            <label for="inputRespiration" class="form-label">Respiration</label>
                        </div>
                        <div class="col-sm-12 col-lg-4">
                            <input type="number" class="form-control" id="inputRespiration" placeholder="Respiration"
                                   min="0" step="0.01" asp-for="Respiration">
                        </div>
                        <div class="col-sm-12 col-lg-5">
                            <input class="form-control" placeholder="Notes" type="text" />
                        </div>
                    </div>
                    <div class="form-group row">
                        <div class="col-sm col-lg-3"></div>
                        <div class="col-sm-12 col-lg-2">
                            <select class="form-control">
                                <option>Method</option>
                            </select>
                        </div>
                        <div class="col-sm col-lg"></div>
                    </div>
                </div>
            </div>
        </div>

        <!-- Pain tab -->
        <div class="tab-pane" id="painTab" role="tabpanel" aria-labelledby="pain-tab">
            <div class="card">
                <div class="card-header bg-primary text-white">
                    <h4>Patient Care Assessment - Pain<span class="float-right">Page 2</span></h4>
                </div>
            </div>

            <div class="row" id="selectPainScale">
                <div class="radio-toolbar">
                    <table class="table">
                        <tbody>
                            <tr>
                                <td><input type="radio" class="painAssessment" name="selectPain" id="selectWongBakerPain" value="WongBaker"><label for="selectWongBakerPain">&nbsp;Select WongBaker FACES&nbsp;</label></td>
                                <td><input type="radio" class="painAssessment" name="selectPain" id="selectCries" value="Cries"><label for="selectCries">&nbsp;Select CRIES&nbsp;</label></td>
                                <td><input type="radio" class="painAssessment" name="selectPain" id="selectNonverbalPain" value="Nonverbal"><label for="selectNonverbalPain">&nbsp;Select Nonverbal Assessment&nbsp;</label></td>
                            </tr>
                        </tbody>
                    </table>
                </div>
            </div>

            <div class="accordion" id="painAccordion">
                <!--Wong-Baker pain scale -->
                <div class="card">
                    <div class="card-header" id="WongBaker">
                        <h2 class="mb-0">
                            <!-- removed data-toggle="collapse" to prevent on-click opening (use selection buttons above instead -->
                            <button class="btn btn-link" type="button" data-target="#collapseWongBaker" aria-expanded="true" aria-controls="collapseWongBaker">
                                Wong-Baker Faces Scale
                            </button>
                            <button type="button" class="btn btn-primary iconButton" data-toggle="modal" data-target="#PcaInfoModal" data-whatever="WongBaker">
                                <i class="fas fa-info"></i>
                            </button>
                        </h2>
                    </div>
                    <div id="collapseWongBaker" class="collapse" aria-labelledby="WongBaker" data-parent="#painAccordion">
                        <div class="card-body">
                            <div class="row">
                                <div class="radio-toolbar">
                                    <table class="table">
                                        <tbody>
                                            <tr>
                                                <td><input type="radio" name="WongBakerPain" id="WongBakerPainNA" checked><label for="WongBakerPainNA">NA</label></td>
                                                <td><input type="radio" name="WongBakerPain" id="hurt0"><label for="hurt0">0 - No Hurt</label></td>
                                                <td><input type="radio" name="WongBakerPain" id="hurt2"><label for="hurt2">2 - Hurts a Little</label></td>
                                                <td><input type="radio" name="WongBakerPain" id="hurt4"><label for="hurt4">4 - Hurts a Little More</label></td>
                                                <td><input type="radio" name="WongBakerPain" id="hurt6"><label for="hurt6">6 - Hurts Even More</label></td>
                                                <td><input type="radio" name="WongBakerPain" id="hurt8"><label for="hurt8">8 - Hurts a Whole Lot</label></td>
                                                <td><input type="radio" name="WongBakerPain" id="hurt10" onclick="ShowHide('hurt10', 'wongBaker')"><label for="hurt10">10 - Hurts Worst</label></td>
                                            </tr>
                                        </tbody>
                                    </table>
                                </div>
                                <div class="form-group">
                                    <label><textarea id="wongBaker" placeholder="Describe" rows="1"></textarea></label>
                                </div>
                            </div>
                            <div class="row">
                                <figure>
                                    <img src="~/img/FACES_English_Black.jpg" alt="Pain Scale 0 to 10">
                                    <figcaption>
                                        FACES licensed to Angela Lee February, 2020
                                    </figcaption>
                                </figure>
                            </div>

                        </div>
                    </div>
                </div>
                <!-- Cries pain scale -->
                <div class="card">
                    <div class="card-header" id="headingCries">
                        <h2 class="mb-0">
                            <button class="btn btn-link" type="button" data-target="#collapseCries" aria-expanded="true" aria-controls="collapseCries">
                                CRIES Scale
                            </button>
                            <button type="button" class="btn btn-primary iconButton" data-toggle="modal" data-target="#PcaInfoModal" data-whatever="cries">
                                <i class="fas fa-info"></i>
                            </button>
                        </h2>
                    </div>
                    <div id="collapseCries" class="collapse" aria-labelledby="headingCries" data-parent="#painAccordion">
                        <div class="card-body">
                            <div class="row">
                                <div class="col-md" title="Characteristic cry of pain is high pitched.">
                                    &nbsp;Crying:
                                    <button type="button" class="btn btn-primary iconButton" data-toggle="modal" data-target="#PcaInfoModal" data-whatever="criesCrying">
                                        <i class="fas fa-info"></i>
                                    </button>
                                </div>
                                <div class="radio-toolbar col-6">
                                    <table class="table">
                                        <tbody>
                                            <tr>
                                                <td><input type="radio" name="crying" id="cryingNA" checked><label for="cryingNA">NA</label></td>
                                                <td><input type="radio" name="crying" id="crying0"><label for="crying0">0 - None</label></td>
                                                <td><input type="radio" name="crying" id="crying1"><label for="crying1">1 - Consolable</label></td>
                                                <td><input type="radio" name="crying" id="crying2"  onclick="ShowHide('crying2', 'criesCrying')"><label for="crying2">2 - Not Consolable</label></td>
                                            </tr>
                                        </tbody>
                                    </table>
                                </div>
                                <div class="form-group col-md">
                                    <textarea id="criesCrying" placeholder="Describe" rows="1"></textarea>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md" title="Requires O2 for SaO2 < 95%: Babies experiencing pain manifest decreased oxygenation. Consider other causes of hypoxemia, e.g., oversedation, atelectasis, pneumothorax.">
                                    &nbsp;Requires O2:
                                    <button type="button" class="btn btn-primary iconButton" data-toggle="modal" data-target="#PcaInfoModal" data-whatever="criesO2">
                                        <i class="fas fa-info"></i>
                                    </button>
                                </div>
                                <div class="radio-toolbar col-6">
                                    <table class="table">
                                        <tbody>
                                            <tr>
                                                <td><input type="radio" name="O2" id="O2NA" checked><label for="O2NA">NA</label></td>
                                                <td><input type="radio" name="O2" id="O20"><label for="O20">0 - None</label></td>
                                                <td><input type="radio" name="O2" id="O21"><label for="O21">1 - &lt;30%</label></td>
                                                <td><input type="radio" name="O2" id="O22"  onclick="ShowHide('O22', 'criesO2')"><label for="O22">2 - &gt;30%</label></td>
                                            </tr>
                                        </tbody>
                                    </table>
                                </div>
                                <div class="form-group col-md">
                                    <label><textarea id="criesO2" placeholder="Describe" rows="1"></textarea></label>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md" title="Increased Vital Signs (BP & HR): Take BP last as this may awaken child making other assessments difficult">
                                    &nbsp;Increased Vitals:
                                    <button type="button" class="btn btn-primary iconButton" data-toggle="modal" data-target="#PcaInfoModal" data-whatever="criesVitalIncrease">
                                        <i class="fas fa-info"></i>
                                    </button>
                                </div>
                                <div class="radio-toolbar col-6">
                                    <table class="table">
                                        <tbody>
                                            <tr>
                                                <td><input type="radio" name="vitalIncrease" id="vitalIncreaseNA" checked><label for="vitalIncreaseNA">NA</label></td>
                                                <td><input type="radio" name="vitalIncrease" id="vitalIncrease0"><label for="vitalIncrease0">0 - None</label></td>
                                                <td><input type="radio" name="vitalIncrease" id="vitalIncrease1"><label for="vitalIncrease1">1 - &lt;20% </label></td>
                                                <td><input type="radio" name="vitalIncrease" id="vitalIncrease2"  onclick="ShowHide('vitalIncrease2', 'criesVitalIncrease')"><label for="vitalIncrease2">2 - &gt;20%</label></td>
                                            </tr>
                                        </tbody>
                                    </table>
                                </div>
                                <div class="form-group col-sm">
                                    <label><textarea id="criesVitalIncrease" placeholder="Describe" rows="1"></textarea></label>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md" title="The facial expression most often associated with pain is a grimace. A grimace may be characterized by brow lowering, eyes squeezed shut, deepening naso-labial furrow, or open lips and mouth">
                                    &nbsp;Expression:
                                    <button type="button" class="btn btn-primary iconButton" data-toggle="modal" data-target="#PcaInfoModal" data-whatever="criesExpression">
                                        <i class="fas fa-info"></i>
                                    </button>
                                </div>
                                <div class="radio-toolbar col-6">
                                    <table class="table">
                                        <tbody>
                                            <tr>
                                                <td><input type="radio" name="Expression" id="expressionNA" checked><label for="expressionNA">NA</label></td>
                                                <td><input type="radio" name="Expression" id="expression0"><label for="expression0">0 - No grimace</label></td>
                                                <td><input type="radio" name="Expression" id="expression1"><label for="expression1">1 - Grimace only</label></td>
                                                <td><input type="radio" name="Expression" id="expression2"  onclick="ShowHide('expression2', 'criesExpression')"><label for="expression2">2 - Added grunt</label></td>
                                            </tr>
                                        </tbody>
                                    </table>
                                </div>
                                <div class="form-group col-sm">
                                    <label><textarea id="criesExpression" placeholder="Describe" rows="1"></textarea></label>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md" title="Scored based upon the infant's state during the hour preceding this recorded score.">
                                    &nbsp;Sleepless:
                                    <button type="button" class="btn btn-primary iconButton" data-toggle="modal" data-target="#PcaInfoModal" data-whatever="criesSleepless">
                                        <i class="fas fa-info"></i>
                                    </button>
                                </div>
                                <div class="radio-toolbar col-6">
                                    <table class="table">
                                        <tbody>
                                            <tr>
                                                <td><input type="radio" name="Sleepless" id="sleeplessNA" checked><label for="sleeplessNA">NA</label></td>
                                                <td><input type="radio" name="Sleepless" id="sleepless0"><label for="sleepless0">0 - Unbroken sleep</label></td>
                                                <td><input type="radio" name="Sleepless" id="sleepless1"><label for="sleepless1">1 - Broken sleep</label></td>
                                                <td><input type="radio" name="Sleepless" id="sleepless2"  onclick="ShowHide('sleepless2', 'criesSleepless')"><label for="sleepless2">2 - Awake</label></td>
                                            </tr>
                                        </tbody>
                                    </table>
                                </div>

                                <div class="form-group col-sm">
                                    <label><textarea id="criesSleepless" placeholder="Describe" rows="1"></textarea></label>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- Nonverbal pain assessment -->
                <div class="card">
                    <div class="card-header" id="headingNonverbal">
                        <h2 class="mb-0">
                            <button class="btn btn-link" type="button" data-target="#collapseNonverbal" aria-expanded="true" aria-controls="collapseNonverbal">
                                Nonverbal pain assessment
                            </button>
                            <button type="button" class="btn btn-primary iconButton" data-toggle="modal" data-target="#PcaInfoModal" data-whatever="nonVerbal">
                                <i class="fas fa-info"></i>
                            </button>
                        </h2>
                    </div>
                    <div id="collapseNonverbal" class="collapse" aria-labelledby="headingNonverbal" data-parent="#painAccordion">
                        <div class="card-body">
                            <div class="row">
                                <div class="col-md" title="Expression">
                                    &nbsp;Face:
                                    <button type="button" class="btn btn-primary iconButton" data-toggle="modal" data-target="#PcaInfoModal" data-whatever="nonVerbalFace">
                                        <i class="fas fa-info"></i>
                                    </button>
                                </div>
                                <div class="radio-toolbar col-7">
                                    <table class="table">
                                        <tbody>
                                            <tr>
                                                <td><input type="radio" name="Face" id="faceNA" checked><label for="faceNA">NA</label></td>
