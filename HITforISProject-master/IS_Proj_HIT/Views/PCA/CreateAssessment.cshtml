@model Pcarecord;
@{
    ViewData["Title"] = "Patient Care Assessment";
}

<nav class="navbar navbar-expand-lg navbar-dark bg-secondary nav-fill w-100 widely">
    <ul class="navbar-nav nav-fill w-100">
        <li class="nav-item menu-item">
            <button class="btn btn-danger" type="button">
                <h4>Patient:&nbsp;@ViewBag.Patient.FirstName&nbsp;@ViewBag.Patient.MiddleName&nbsp;@ViewBag.Patient.LastName</h4>
            </button>
        </li>
        <li class="nav-item menu-item dob patientBanner">
            <span class="heading">DOB: </span> @ViewBag.Patient.Dob.ToShortDateString()
        </li>
        <li class="nav-item menu-item patientBanner">
            <span class="heading">Age: </span> @ViewBag.Encounter.PatientCurrentAge
        </li>
        <li class="nav-item menu-item patientBanner">
            <span class="heading">MRN: </span>@(ViewBag.Encounter.Mrn ?? string.Empty)
        </li>
    </ul>
</nav>
<form class="col" method="post" id="pca" name="pca" asp-action="SaveAssessment">
    <input type="hidden" asp-for="EncounterId" value="@ViewBag.Encounter.EncounterId" />
    <div class="tab-content">
        <!-- General tab -->
        <div class="tab-pane active" id="general" role="tabpanel" aria-labelledby="general-tab">

            <div class="card">
                <div class="card-header bg-primary text-white">
                    <h4>PCA-General Systems <span class="float-right">Page 1</span></h4>
                </div>
            </div>            
            <div class="row">
                <div class="col-md">
                    <div class="card">
                        <div class="card-header">
                            <div class="form-group row">
                                <label for="inputHeight" class="col-md col-form-label">Height in Inches</label>
                                <div class="col-md">
                                    <input type="number" class="form-control" id="inputHeight">
                                </div>
                            </div>
                            <div class="form-group row">
                                <label for="inputWeight" class="col-md col-form-label">Weight in pounds</label>
                                <div class="col-md">
                                    <input type="number" class="form-control" id="inputWeight">
                                </div>
                            </div>
                            <div class="form-group row">
                                <label for="inputBMI" class="col-md col-form-label">Body MassIndex</label>
                                <div class="col-md">
                                    <input type="number" class="form-control" id="inputBMI">
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="card">
                        <div class="card-header">
                            <div class="form-group row">
                                <label for="inputTemperature" class="col-md col-form-label">Temperature (fahrenheit)</label>
                                <div class="col-md">
                                    <input type="number" class="form-control" id="inputTemperature">
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="card">
                        <div class="card-header">
                            <div class="form-group row">
                                <label for="inputHead" class="col-md col-form-label">Head Circumference</label>
                                <div class="col-md">
                                    <input type="number" class="form-control" id="inputHead">
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-md">
                    <div class="card">
                        <div class="card-header">
                            Blood Pressure mmHg
                            <div class="form-group row">
                                <label for="inputCystolic" class="col-md col-form-label">Cystolic</label>
                                <div class="col-md">
                                    <input type="number" class="form-control" id="inputCystolic">
                                </div>
                            </div>
                            <div class="form-group row">
                                <label for="inputDiastolic" class="col-md col-form-label">Diastolic</label>
                                <div class="col-md">
                                    <input type="number" class="form-control" id="inputDiastolic">
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="card">
                        <div class="card-header">
                            <div class="form-group row">
                                <label for="inputPulse" class="col-md col-form-label">Pulse</label>
                                <div class="col-md">
                                    <input type="number" class="form-control" id="inputPulse">
                                </div>
                            </div>
                            <div class="form-group row">
                                <label for="inputPulseOxi" class="col-md col-form-label">Pulse Oximentry</label>
                                <div class="col-md">
                                    <input type="number" class="form-control" id="inputPulseOxi">
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <!-- Pain tab -->
        <!-- TODO: Once one tool is chosen, lock out the other two -->
        <div class="tab-pane" id="painTab" role="tabpanel" aria-labelledby="pain-tab">
            <div class="card">
                <div class="card-header bg-primary text-white">
                    <h4>PCA-Pain Assessment<span class="float-right">Page 2</span></h4>
                </div>
            </div>
            <div class="accordion" id="accordion">
                <!--Wong-Baker pain scale -->
                <div class="card">
                    <div class="card-header" id="WongBaker">
                        <h2 class="mb-0">
                            <button class="btn btn-link" type="button" data-toggle="collapse" data-target="#collapseWongBaker" aria-expanded="true" aria-controls="collapseWongBaker" title="">
                                Wong-Baker Faces Scale
                            </button>
                            <button type="button" class="btn btn-primary iconButton" data-toggle="modal" data-target="#PcaInfoModal" data-whatever="WongBaker">
                                <i class="fas fa-info"></i>
                            </button>
                        </h2>
                    </div>
                    <div id="collapseWongBaker" class="collapse" aria-labelledby="WongBaker" data-parent="#accordion">
                        <div class="card-body">
                            <div class="row">
                                <figure>
                                    <img src="~/img/FACES_English_Black.jpg" alt="Pain Scale 0 to 10">
                                    <figcaption>
                                        FACES licensed to Angela Lee February, 2020
                                    </figcaption>
                                </figure>
                            </div>
                            <div class="row">
                                <div class="radio-toolbar">
                                    <table class="table">
                                        <tbody>
                                            <tr>
                                                <td><input type="radio" name="WongBakerPain" id="WongBakerPainNA" checked><label for="WongBakerPainNA">NA</label></td>
                                                <td><input type="radio" name="WongBakerPain" id="Hurt0"><label for="hurt0">0 - No Hurt</label></td>
                                                <td><input type="radio" name="WongBakerPain" id="hurt2"><label for="hurt2">2 - Hurts a Little</label></td>
                                                <td><input type="radio" name="WongBakerPain" id="hurt4"><label for="hurt4">4 - Hurts a Little More</label></td>
                                                <td><input type="radio" name="WongBakerPain" id="hurt6"><label for="hurt6">6 - Hurts Even More</label></td>
                                                <td><input type="radio" name="WongBakerPain" id="hurt8"><label for="hurt8">8 - Hurts a Whole Lot</label></td>
                                                <td><input type="radio" name="WongBakerPain" id="hurt10"><label for="hurt10">10 - Hurts Worst</label></td>
                                            </tr>
                                        </tbody>
                                    </table>
                                </div>
                                <div class="form-group">
                                    <label><textarea id="wongBaker" placeholder="Describe" rows="1"></textarea></label>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- Cries pain scale -->
                <div class="card">
                    <div class="card-header" id="headingCries">
                        <h2 class="mb-0">
                            <button class="btn btn-link" type="button" data-toggle="collapse" data-target="#collapseCries" aria-expanded="true" aria-controls="collapseCries">
                                CRIES Scale
                            </button>
                            <button type="button" class="btn btn-primary iconButton" data-toggle="modal" data-target="#PcaInfoModal" data-whatever="cries">
                                <i class="fas fa-info"></i>
                            </button>
                        </h2>
                    </div>
                    <div id="collapseCries" class="collapse" aria-labelledby="headingCries" data-parent="#accordion">
                        <div class="card-body">
                            <div class="row">
                                <div class="col-md" title="Characteristic cry of pain is high pitched.">
                                    &nbsp;Crying:
                                    <button type="button" class="btn btn-primary iconButton" data-toggle="modal" data-target="#PcaInfoModal" data-whatever="criesCrying">
                                        <i class="fas fa-info"></i>
                                    </button>
                                </div>
                                <div class="radio-toolbar col-6">
                                    <table class="table">
                                        <tbody>
                                            <tr>
                                                <td><input type="radio" name="crying" id="cryingNA" checked><label for="cryingNA">NA</label></td>
                                                <td><input type="radio" name="crying" id="crying0"><label for="crying0">0 - None</label></td>
                                                <td><input type="radio" name="crying" id="crying1"><label for="crying1">1 - Consolable</label></td>
                                                <td><input type="radio" name="crying" id="crying2"><label for="crying2">2 - Not Consolable</label></td>
                                            </tr>
                                        </tbody>
                                    </table>
                                </div>
                                <div class="form-group col-md">
                                    <textarea id="criesCrying" placeholder="Describe" rows="1"></textarea>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md" title="Requires O2 for SaO2 < 95%: Babies experiencing pain manifest decreased oxygenation. Consider other causes of hypoxemia, e.g., oversedation, atelectasis, pneumothorax.">
                                    &nbsp;Requires O2:
                                    <button type="button" class="btn btn-primary iconButton" data-toggle="modal" data-target="#PcaInfoModal" data-whatever="criesO2">
                                        <i class="fas fa-info"></i>
                                    </button>
                                </div>
                                <div class="radio-toolbar col-6">
                                    <table class="table">
                                        <tbody>
                                            <tr>
                                                <td><input type="radio" name="O2" id="O2NA" checked><label for="O2NA">NA</label></td>
                                                <td><input type="radio" name="O2" id="O20"><label for="O20">0 - None</label></td>
                                                <td><input type="radio" name="O2" id="O21"><label for="O21">1 - &lt;30%</label></td>
                                                <td><input type="radio" name="O2" id="O22"><label for="O22">2 - &gt;30%</label></td>
                                            </tr>
                                        </tbody>
                                    </table>                                    
                                </div>
                                <div class="form-group col-md">
                                    <label><textarea id="criesO2" placeholder="Describe" rows="1"></textarea></label>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md" title="Increased Vital Signs (BP & HR): Take BP last as this may awaken child making other assessments difficult">
                                    &nbsp;Increased Vitals:
                                    <button type="button" class="btn btn-primary iconButton" data-toggle="modal" data-target="#PcaInfoModal" data-whatever="criesVitalIncrease">
                                        <i class="fas fa-info"></i>
                                    </button>
                                </div>
                                <div class="radio-toolbar col-6">
                                    <table class="table">
                                        <tbody>
                                            <tr>
                                                <td><input type="radio" name="vitalIncrease" id="vitalIncreaseNA" checked><label for="vitalIncreaseNA">NA</label></td>
                                                <td><input type="radio" name="vitalIncrease" id="vitalIncrease0"><label for="vitalIncrease0">0 - None</label></td>
                                                <td><input type="radio" name="vitalIncrease" id="vitalIncrease1"><label for="vitalIncrease1">1 - &lt;20% </label></td>
                                                <td><input type="radio" name="vitalIncrease" id="vitalIncrease2"><label for="vitalIncrease2">2 - &gt;20%</label></td>
                                            </tr>
                                        </tbody>
                                    </table>
                                </div>
                                <div class="form-group col-sm">
                                    <label><textarea id="criesIncreasedVital" placeholder="Describe" rows="1"></textarea></label>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md" title="The facial expression most often associated with pain is a grimace. A grimace may be characterized by brow lowering, eyes squeezed shut, deepening naso-labial furrow, or open lips and mouth">
                                    &nbsp;Expression:
                                    <button type="button" class="btn btn-primary iconButton" data-toggle="modal" data-target="#PcaInfoModal" data-whatever="criesExpression">
                                        <i class="fas fa-info"></i>
                                    </button>
                                </div>
                                <div class="radio-toolbar col-6">
                                    <table class="table">
                                        <tbody>
                                            <tr>
                                                <td><input type="radio" name="Expression" id="expressionNA" checked><label for="expressionNA">NA</label></td>
                                                <td><input type="radio" name="Expression" id="expression0"><label for="expression0">0 - No grimace</label></td>
                                                <td><input type="radio" name="Expression" id="expression1"><label for="expression1">1 - Grimace only</label></td>
                                                <td><input type="radio" name="Expression" id="expression2"><label for="expression2">2 - Added grunt</label></td>
                                            </tr>
                                        </tbody>
                                    </table>
                                </div>
                                <div class="form-group col-sm">
                                    <label><textarea id="criesExpression" placeholder="Describe" rows="1"></textarea></label>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md" title="Scored based upon the infant's state during the hour preceding this recorded score.">
                                    &nbsp;Sleepless:
                                    <button type="button" class="btn btn-primary iconButton" data-toggle="modal" data-target="#PcaInfoModal" data-whatever="criesSleepless">
                                        <i class="fas fa-info"></i>
                                    </button>
                                </div>
                                <div class="radio-toolbar col-6">
                                    <table class="table">
                                        <tbody>
                                            <tr>
                                                <td><input type="radio" name="Sleepless" id="sleeplessNA" checked><label for="sleeplessNA">NA</label></td>
                                                <td><input type="radio" name="Sleepless" id="sleepless0"><label for="sleepless0">0 - Unbroken sleep</label></td>
                                                <td><input type="radio" name="Sleepless" id="sleepless1"><label for="sleepless1">1 - Broken sleep</label></td>
                                                <td><input type="radio" name="Sleepless" id="sleepless2"><label for="sleepless2">2 - Awake</label></td>
                                            </tr>
                                        </tbody>
                                    </table>
                                </div>

                                <div class="form-group col-sm">
                                    <label><textarea id="criesSleepless" placeholder="Describe" rows="1"></textarea></label>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- Nonverbal pain assessment -->
                <div class="card">
                    <div class="card-header" id="headingNonverbal">
                        <h2 class="mb-0">
                            <button class="btn btn-link" type="button" data-toggle="collapse" data-target="#collapseNonverbal" aria-expanded="true" aria-controls="collapseNonverbal">
                                Nonverbal pain assessment
                            </button>
                            <button type="button" class="btn btn-primary iconButton" data-toggle="modal" data-target="#PcaInfoModal" data-whatever="nonVerbal">
                                <i class="fas fa-info"></i>
                            </button>
                        </h2>
                    </div>
                    <div id="collapseNonverbal" class="collapse" aria-labelledby="headingNonverbal" data-parent="#accordion">
                        <div class="card-body">
                            <div class="row">
                                <div class="col-md" title="Expression">
                                    &nbsp;Face:
                                    <button type="button" class="btn btn-primary iconButton" data-toggle="modal" data-target="#PcaInfoModal" data-whatever="nonVerbalFace">
                                        <i class="fas fa-info"></i>
                                    </button>
                                </div>
                                <div class="radio-toolbar col-7">
                                    <table class="table">
                                        <tbody>
                                            <tr>
                                                <td><input type="radio" name="Face" id="faceNA" checked><label for="faceNA">NA</label></td>
                                                <td><input type="radio" name="Face" id="face0"><label for="face0">0 - None</label></td>
                                                <td><input type="radio" name="Face" id="face1"><label for="face1">1 - Occasional</label></td>
                                                <td><input type="radio" name="Face" id="face2"><label for="face2">2 - Frequent</label></td>
                                            </tr>
                                        </tbody>
                                    </table>
                                </div>
                                <div class="form-group col-sm">
                                    <label><textarea id="nonverbalFace" placeholder="Describe" rows="1"></textarea></label>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md" title="Movement">
                                    &nbsp;Activity:
                                    <button type="button" class="btn btn-primary iconButton" data-toggle="modal" data-target="#PcaInfoModal" data-whatever="nonVerbalMovement">
                                        <i class="fas fa-info"></i>
                                    </button>
                                </div>
                                <div class="radio-toolbar col-7">
                                    <table class="table">
                                        <tbody>
                                            <tr>
                                                <td><input type="radio" name="Movement" id="movementNA" checked><label for="movementNA">NA</label></td>
                                                <td><input type="radio" name="Movement" id="movement0"><label for="movement0">0 - Quiet</label></td>
                                                <td><input type="radio" name="Movement" id="movement1"><label for="movement1">1 - Some Movement</label></td>
                                                <td><input type="radio" name="Movement" id="movement2"><label for="movement2">2 - Restless/ withdrawn</label></td>
                                            </tr>
                                        </tbody>
                                    </table>
                                </div>
                                <div class="form-group col-sm">
                                    <label><textarea id="nonverbalMovement" placeholder="Describe" rows="1"></textarea></label>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md" title="Guarding the body or reluctance to move">
                                    &nbsp;Guarding:
                                    <button type="button" class="btn btn-primary iconButton" data-toggle="modal" data-target="#PcaInfoModal" data-whatever="nonVerbalGuarding">
                                        <i class="fas fa-info"></i>
                                    </button>
                                </div>
                                <div class="radio-toolbar col-7">
                                    <table class="table">
                                        <tbody>
                                            <tr>
                                                <td><input type="radio" name="Guarding" id="guardingNA" checked><label for="guardingNA">NA</label></td>
                                                <td><input type="radio" name="Guarding" id="guarding0"><label for="guarding0">0 - None</label></td>
                                                <td><input type="radio" name="Guarding" id="guarding1"><label for="guarding1">1 - Splinting</label></td>
                                                <td><input type="radio" name="Guarding" id="guarding2"><label for="guarding2">2 - Rigid</label></td>
                                            </tr>
                                        </tbody>
                                    </table>
                                </div>
                                <div class="form-group col-sm">
                                    <label><textarea id="nonverbalGuarding" placeholder="Describe" rows="1"></textarea></label>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md" title="Vital signs">
                                    &nbsp;Physiology:
                                    <button type="button" class="btn btn-primary iconButton" data-toggle="modal" data-target="#PcaInfoModal" data-whatever="nonVerbalPhysiology">
                                        <i class="fas fa-info"></i>
                                    </button>
                                </div>
                                <div class="radio-toolbar col-7">
                                    <table class="table">
                                        <tbody>
                                            <tr>
                                                <td><input type="radio" name="Physiology" id="physiologyNA" checked><label for="physiologyNA">NA</label></td>
                                                <td><input type="radio" name="Physiology" id="physiology0"><label for="physiology0">0 - Unchanged</label></td>
                                                <td><input type="radio" name="Physiology" id="physiology1"><label for="physiology1">1 - Some change</label></td>
                                                <td><input type="radio" name="Physiology" id="physiology2"><label for="physiology2">2 - Definite change</label></td>
                                            </tr>
                                        </tbody>
                                    </table>
                                </div>
                                <div class="form-group col-sm">
                                    <label><textarea id="nonverbalPhysiology" placeholder="Describe" rows="1"></textarea></label>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md" title="Respiration">
                                    &nbsp;Respiratory:
                                    <button type="button" class="btn btn-primary iconButton" data-toggle="modal" data-target="#PcaInfoModal" data-whatever="nonVerbalRespiratory">
                                        <i class="fas fa-info"></i>
                                    </button>
                                </div>
                                <div class="radio-toolbar col-7">
                                    <table class="table">
                                        <tbody>
                                            <tr>
                                                <td><input type="radio" name="Respiratory" id="respiratoryNA" checked><label for="respiratoryNA">NA</label></td>
                                                <td><input type="radio" name="Respiratory" id="respiratory0"><label for="respiratory0">0 - Baseline</label></td>
                                                <td><input type="radio" name="Respiratory" id="respiratory1"><label for="respiratory1">1 - Some change</label></td>
                                                <td><input type="radio" name="Respiratory" id="respiratory2"><label for="respiratory2">2 - Definite change</label></td>
                                            </tr>
                                        </tbody>
                                    </table>
                                </div>
                                <div class="form-group col-sm">
                                    <label><textarea id="nonverbalRespiratory" placeholder="Describe" rows="1"></textarea></label>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <!-- Vitals tab -->
        <div class="tab-pane" id="secondary" role="tabpanel" aria-labelledby="secondary-tab">
            <div class="card">
                <div class="card-header bg-primary text-white">
                    <h4>PCA-Secondary Systems<span class="float-right">Page 3</span></h4>
                </div>
            </div>
            <div class="accordion" id="accordion">
                <div class="card">
                    <div class="card-header" id="headingOne">
                        <h2 class="mb-0">
                            <button class="btn btn-link collapsed" type="button" data-toggle="collapse" data-target="#collapseOne" aria-expanded="false" aria-controls="collapseOne">
                                Level of Consciousness and Orientation
                            </button>
                        </h2>
                    </div>
                    <div id="collapseOne" class="collapse show" aria-labelledby="headingOne" data-parent="#accordion">
                        <div class="card-body ">
                            <div class="row m-2">
                                <div class="col-3 mb-2">
                                    Person:
                                </div>
                                <div class="radio-toolbar col-6">
                                    <table class="table">
                                        <tbody>
                                            <tr>
                                                <td><input type="radio" name="Person" onclick="ShowHide('person', 'personArea')" value="notAssessed" id="personNA" checked><label for="personNA">Not Assessed</label></td>
                                                <td><input type="radio" name="Person" onclick="ShowHide('person', 'personArea')" value="wnl" id="personWnl"><label for="personWnl">WNL</label></td>
                                                <td><input type="radio" name="Person" onclick="ShowHide('person', 'personArea')" id="person" value="abnormal"><label for="person">Abnormal</label></td>
                                            </tr>
                                        </tbody>
                                    </table>
                                </div>
                                <div class="form-group col m-0">
                                    <label><textarea class="d-none" id="personArea" placeholder="Describe" rows="1"></textarea></label>
                                </div>
                            </div>
                            <div class="row m-2">
                                <div class="col-3 mb-2">
                                    Place:
                                </div>
                                <div class="radio-toolbar col-6">
                                    <table class="table">
                                        <tbody>
                                            <tr>
                                                <td><input type="radio" name="Place" onclick="ShowHide('place', 'placeArea')" value="notAssessed" id="placeNA" checked><label for="placeNA">Not Assessed</label></td>
                                                <td><input type="radio" name="Place" onclick="ShowHide('place', 'placeArea')" value="wnl" id="placeWnl"><label for="placeWnl">WNL</label></td>
                                                <td><input type="radio" name="Place" onclick="ShowHide('place', 'placeArea')" id="place" value="abnormal"><label for="place">Abnormal</label></td>
                                            </tr>
                                        </tbody>
                                    </table>
                                </div>
                                <div class="form-group col m-0">
                                    <label><textarea class="d-none" id="placeArea" placeholder="Describe" rows="1"></textarea></label>
                                </div>
                            </div>
                            <div class="row m-2">
                                <div class="col-3 mb-2">
                                    Time:
                                </div>
                                <div class="radio-toolbar col-6">
                                    <table class="table">
                                        <tbody>
                                            <tr>
